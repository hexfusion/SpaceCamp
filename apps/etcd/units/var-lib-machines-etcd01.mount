[Unit]
Description=Etcd instance 01 nspawn container storage
ConditionPathExists=/var/lib/container/base/jessie_amd64
ConditionPathExists=/var/lib/container/etcd01/rw
ConditionPathExists=/var/lib/container/etcd01/work

[Mount]
What=overlay
Where=/var/lib/machines/etcd01
Type=overlay
Options=lowerdir=/var/lib/container/base/jessie_amd64,upperdir=/var/lib/container/etcd01/rw,workdir=/var/lib/container/etcd01/work

[Install]
WantedBy = multi-user.target
